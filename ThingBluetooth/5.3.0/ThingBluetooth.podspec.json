{
  "name": "ThingBluetooth",
  "version": "5.3.0",
  "summary": "Tuya bluetooth, less write and can use anywhere.",
  "homepage": "https://github.com/tuya",
  "license": "none",
  "authors": {
    "TuyaInc": "https://developer.tuya.com/"
  },
  "source": {
